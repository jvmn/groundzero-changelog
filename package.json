{
  "name": "@jvmn/groundzero-changelog",
  "version": "1.6.0",
  "description": "Changelog generator for JvM/Neckar Groundzero projects",
  "keywords": [
    "changelog",
    "groundzero"
  ],
  "author": "Jung von Matt/Neckar - Shachar Leuchter",
  "license": "MIT",
  "engines": {
    "node": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jvmn/groundzero-changelog"
  },
  "main": "./lib/index.js",
  "bin": {
    "groundzero-changelog": "./cli.js"
  },
  "jvmChangelog": {
    "gitRepo": "https://github.com/jvmn/groundzero-changelog",
    "gitPlatform": "github",
    "ignores": [
      "build",
      "revert",
      "test"
    ],
    "extraComments": [
      "Breaking changes"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "release": "node ./lib/index.js"
  },
  "lint-staged": {
    "**/*.js": "eslint"
  },
  "dependencies": {
    "nodegit": "^0.23.0",
    "path": "^0.12.7",
    "semver": "^5.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.0.1",
    "eslint": "^5.8.0",
    "lint-staged": "^8.0.4",
    "husky": "^1.1.2"
  }
}
