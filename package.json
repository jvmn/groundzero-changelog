{
  "name": "@jvmn/groundzero-changelog",
  "version": "1.3.0",
  "description": "Changelog generator for JvM/Neckar Groundzero projects",
  "keywords": [
    "changelog",
    "groundzero"
  ],
  "author": "Jung von Matt/Neckar - Shachar Leuchter",
  "license": "MIT",
  "engines": {
    "node": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://stash.jvm.de/projects/JVMNEC/repos/groundzero-changelog"
  },
  "main": "./lib/index.js",
  "bin": {
    "groundzero-changelog": "./cli.js"
  },
  "jvmChangelog": {
    "gitRepo": "https://stash.jvm.de/projects/JVMNEC/repos/groundzero-changelog",
    "ignores": "build,revert,test"
  },
  "scripts": {
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "release": "node ./lib/index.js"
  },
  "lint-staged": {
    "**/*.js": "eslint"
  },
  "dependencies": {
    "nodegit": "^0.22.2",
    "path": "^0.12.7",
    "semver": "^5.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "eslint": "^5.3.0",
    "lint-staged": "^7.2.0",
    "husky": "^0.14.3"
  }
}
